{
  "name": "js-assessment",
  "description": "A test-driven assessment of JavaScript skills",
  "version": "0.4.0",
  "repository": "git://github.com/nikorablin/js-assessment.git",
  "engines": {
    "node": "^18"
  },
  "type": "module",
  "dependencies": {},
  "scripts": {
    "start": "node index.js",
    "test": "node --no-warnings --experimental-vm-modules $( [ -f ./node_modules/.bin/jest ] && echo ./node_modules/.bin/jest || which jest ) --watch",
    "lint": "eslint --quiet tests/app app",
    "lint-warn": "eslint tests/app app"
  },
  "private": true,
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.22.5",
    "jest": "^29.5.0"
  },
  "main": "index.js",
  "author": "Nik Korablin <nikorablin@gmail.com>",
  "license": "MIT"
}
